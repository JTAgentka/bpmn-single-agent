<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_ImplementCreator"
                  targetNamespace="http://jtbank.cz/investment">
  
  <bpmn:collaboration id="Collaboration_ImplementCreator">
    <bpmn:participant id="Participant_Creator" name="Product Implementation - Bank as Creator" processRef="Process_Creator"/>
  </bpmn:collaboration>
  
  <bpmn:process id="Process_Creator" isExecutable="true">
    
    <bpmn:laneSet id="LaneSet_Creator">
      <bpmn:lane id="Lane_Product" name="Klient">
        <bpmn:flowNodeRef>StartEvent_Approved</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReviewProduct</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Implemented</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_SystemProc" name="JT Bank Systém">
        <bpmn:flowNodeRef>Task_RiskAnalysis</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_TargetMarket</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_FeeStructure</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SystemConfig</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GenerateKID</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>ParallelGateway_Analysis</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>ParallelGateway_Merge</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_Specialist" name="Bankéř">
        <bpmn:flowNodeRef>Task_CreateProductList</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_DefineDistribution</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PrepareDocuments</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Slovak</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SlovakCompliance</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Approved" name="Product approved for implementation">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Tasks -->
    <bpmn:manualTask id="Task_CreateProductList" name="Create product list">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:manualTask>
    
    <bpmn:parallelGateway id="ParallelGateway_Analysis">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3a</bpmn:outgoing>
      <bpmn:outgoing>Flow_3b</bpmn:outgoing>
      <bpmn:outgoing>Flow_3c</bpmn:outgoing>
    </bpmn:parallelGateway>
    
    <bpmn:serviceTask id="Task_RiskAnalysis" name="Analyze product risks">
      <bpmn:incoming>Flow_3a</bpmn:incoming>
      <bpmn:outgoing>Flow_4a</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="Task_TargetMarket" name="Define target market">
      <bpmn:incoming>Flow_3b</bpmn:incoming>
      <bpmn:outgoing>Flow_4b</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="Task_FeeStructure" name="Configure fee structure">
      <bpmn:incoming>Flow_3c</bpmn:incoming>
      <bpmn:outgoing>Flow_4c</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:parallelGateway id="ParallelGateway_Merge">
      <bpmn:incoming>Flow_4a</bpmn:incoming>
      <bpmn:incoming>Flow_4b</bpmn:incoming>
      <bpmn:incoming>Flow_4c</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:parallelGateway>
    
    <bpmn:manualTask id="Task_DefineDistribution" name="Define distribution strategy">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:manualTask>
    
    <bpmn:serviceTask id="Task_SystemConfig" name="Configure IT systems">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:manualTask id="Task_PrepareDocuments" name="Prepare product documentation">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:manualTask>
    
    <bpmn:serviceTask id="Task_GenerateKID" name="Generate KID documents">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:exclusiveGateway id="Gateway_Slovak" name="Slovak market?">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_10_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_10_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:manualTask id="Task_SlovakCompliance" name="Apply Slovak market compliance">
      <bpmn:incoming>Flow_10_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:manualTask>
    
    <bpmn:userTask id="Task_ReviewProduct" name="Review implemented product">
      <bpmn:incoming>Flow_10_No</bpmn:incoming>
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Implemented" name="Product implemented">
      <bpmn:incoming>Flow_12</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_Approved" targetRef="Task_CreateProductList"/>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_CreateProductList" targetRef="ParallelGateway_Analysis"/>
    <bpmn:sequenceFlow id="Flow_3a" sourceRef="ParallelGateway_Analysis" targetRef="Task_RiskAnalysis"/>
    <bpmn:sequenceFlow id="Flow_3b" sourceRef="ParallelGateway_Analysis" targetRef="Task_TargetMarket"/>
    <bpmn:sequenceFlow id="Flow_3c" sourceRef="ParallelGateway_Analysis" targetRef="Task_FeeStructure"/>
    <bpmn:sequenceFlow id="Flow_4a" sourceRef="Task_RiskAnalysis" targetRef="ParallelGateway_Merge"/>
    <bpmn:sequenceFlow id="Flow_4b" sourceRef="Task_TargetMarket" targetRef="ParallelGateway_Merge"/>
    <bpmn:sequenceFlow id="Flow_4c" sourceRef="Task_FeeStructure" targetRef="ParallelGateway_Merge"/>
    <bpmn:sequenceFlow id="Flow_5" sourceRef="ParallelGateway_Merge" targetRef="Task_DefineDistribution"/>
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_DefineDistribution" targetRef="Task_SystemConfig"/>
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_SystemConfig" targetRef="Task_PrepareDocuments"/>
    <bpmn:sequenceFlow id="Flow_8" sourceRef="Task_PrepareDocuments" targetRef="Task_GenerateKID"/>
    <bpmn:sequenceFlow id="Flow_9" sourceRef="Task_GenerateKID" targetRef="Gateway_Slovak"/>
    <bpmn:sequenceFlow id="Flow_10_Yes" name="Yes" sourceRef="Gateway_Slovak" targetRef="Task_SlovakCompliance"/>
    <bpmn:sequenceFlow id="Flow_10_No" name="No" sourceRef="Gateway_Slovak" targetRef="Task_ReviewProduct"/>
    <bpmn:sequenceFlow id="Flow_11" sourceRef="Task_SlovakCompliance" targetRef="Task_ReviewProduct"/>
    <bpmn:sequenceFlow id="Flow_12" sourceRef="Task_ReviewProduct" targetRef="EndEvent_Implemented"/>
  </bpmn:process>
  
  <!-- DI Information -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Creator">
    <bpmndi:BPMNPlane id="BPMNPlane_Creator" bpmnElement="Collaboration_ImplementCreator">
      
      <!-- Pool -->
      <bpmndi:BPMNShape id="Participant_Creator_di" bpmnElement="Participant_Creator" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="1600" height="600"/>
      </bpmndi:BPMNShape>
      
      <!-- Lanes -->
      <bpmndi:BPMNShape id="Lane_Product_di" bpmnElement="Lane_Product" isHorizontal="true">
        <dc:Bounds x="190" y="80" width="1570" height="200"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_SystemProc_di" bpmnElement="Lane_SystemProc" isHorizontal="true">
        <dc:Bounds x="190" y="280" width="1570" height="200"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_Specialist_di" bpmnElement="Lane_Specialist" isHorizontal="true">
        <dc:Bounds x="190" y="480" width="1570" height="200"/>
      </bpmndi:BPMNShape>
      
      <!-- Elements positioned vertically -->
      <bpmndi:BPMNShape id="StartEvent_Approved_di" bpmnElement="StartEvent_Approved">
        <dc:Bounds x="242" y="162" width="36" height="36"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_CreateProductList_di" bpmnElement="Task_CreateProductList">
        <dc:Bounds x="210" y="540" width="160" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="ParallelGateway_Analysis_di" bpmnElement="ParallelGateway_Analysis">
        <dc:Bounds x="265" y="355" width="50" height="50"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_RiskAnalysis_di" bpmnElement="Task_RiskAnalysis">
        <dc:Bounds x="380" y="290" width="160" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_TargetMarket_di" bpmnElement="Task_TargetMarket">
        <dc:Bounds x="380" y="380" width="160" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_FeeStructure_di" bpmnElement="Task_FeeStructure">
        <dc:Bounds x="560" y="340" width="160" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="ParallelGateway_Merge_di" bpmnElement="ParallelGateway_Merge">
        <dc:Bounds x="785" y="355" width="50" height="50"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_DefineDistribution_di" bpmnElement="Task_DefineDistribution">
        <dc:Bounds x="730" y="540" width="160" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_SystemConfig_di" bpmnElement="Task_SystemConfig">
        <dc:Bounds x="950" y="340" width="160" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_PrepareDocuments_di" bpmnElement="Task_PrepareDocuments">
        <dc:Bounds x="950" y="540" width="160" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_GenerateKID_di" bpmnElement="Task_GenerateKID">
        <dc:Bounds x="1170" y="340" width="160" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_Slovak_di" bpmnElement="Gateway_Slovak" isMarkerVisible="true">
        <dc:Bounds x="1225" y="555" width="50" height="50"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_SlovakCompliance_di" bpmnElement="Task_SlovakCompliance">
        <dc:Bounds x="1340" y="540" width="160" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_ReviewProduct_di" bpmnElement="Task_ReviewProduct">
        <dc:Bounds x="1560" y="140" width="160" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Implemented_di" bpmnElement="EndEvent_Implemented">
        <dc:Bounds x="1642" y="242" width="36" height="36"/>
      </bpmndi:BPMNShape>
      
      <!-- Edges -->
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="260" y="198"/>
        <di:waypoint x="260" y="540"/>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="290" y="540"/>
        <di:waypoint x="290" y="405"/>
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>