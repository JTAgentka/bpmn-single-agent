        <bpmndi:BPMNLabel>
          <dc:Bounds x="1135" y="285" width="66" height="27"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="end_standard_di" bpmnElement="end_standard">
        <dc:Bounds x="1150" y="352" width="36" height="36"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1127" y="395" width="82" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Boundary Event -->
      <bpmndi:BPMNShape id="timeout_completion_di" bpmnElement="timeout_completion">
        <dc:Bounds x="732" y="412" width="36" height="36"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="720" y="455" width="60" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Data Objects -->
      <bpmndi:BPMNShape id="data_initiative_document_di" bpmnElement="data_initiative_document">
        <dc:Bounds x="380" y="25" width="36" height="50"/>
