<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_ProductInitiation"
                  targetNamespace="http://jtbank.cz/bpmn">
  
  <bpmn:collaboration id="Collaboration_ProductInitiation">
    <bpmn:participant id="Pool_ProductInitiation" name="Product Initiation Process" processRef="Process_ProductInitiation"/>
  </bpmn:collaboration>
  
  <bpmn:process id="Process_ProductInitiation" isExecutable="true">
    <!-- Lanes -->
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_Initiator" name="Iniciátor">
        <bpmn:flowNodeRef>start_client_request</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>start_market_opportunity</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>start_new_emission</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>start_unique_idea</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task_document_initiative</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task_submit_initiative</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_ODIC" name="ODIC">
        <bpmn:flowNodeRef>gateway_completeness</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task_request_completion</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>gateway_accelerated</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task_archive_initiative</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task_accelerated_processing</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task_standard_processing</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>end_accelerated</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>end_standard</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>timeout_completion</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    
    <!-- Start Events -->
    <bpmn:startEvent id="start_client_request" name="Klientská poptávka">
      <bpmn:outgoing>flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:startEvent id="start_market_opportunity" name="Tržní příležitost">
      <bpmn:outgoing>flow_2</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:startEvent id="start_new_emission" name="Nová emise">
      <bpmn:outgoing>flow_3</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:startEvent id="start_unique_idea" name="Inovativní nápad">
      <bpmn:outgoing>flow_4</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Tasks -->
    <bpmn:userTask id="task_document_initiative" name="Dokumentovat podnět">
      <bpmn:incoming>flow_1</bpmn:incoming>
      <bpmn:incoming>flow_2</bpmn:incoming>
      <bpmn:incoming>flow_3</bpmn:incoming>
      <bpmn:incoming>flow_4</bpmn:incoming>
      <bpmn:incoming>flow_incomplete</bpmn:incoming>
      <bpmn:outgoing>flow_5</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="task_submit_initiative" name="Podat písemný podnět">
      <bpmn:incoming>flow_5</bpmn:incoming>
      <bpmn:outgoing>flow_6</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Gateways -->
    <bpmn:exclusiveGateway id="gateway_completeness" name="Je podnět kompletní?">
      <bpmn:incoming>flow_6</bpmn:incoming>
      <bpmn:outgoing>flow_complete</bpmn:outgoing>
      <bpmn:outgoing>flow_incomplete_check</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="task_request_completion" name="Vyžádat doplnění">
      <bpmn:incoming>flow_incomplete_check</bpmn:incoming>
      <bpmn:outgoing>flow_incomplete</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:serviceTask id="task_archive_initiative" name="Archivovat podnět">
      <bpmn:incoming>flow_complete</bpmn:incoming>
      <bpmn:outgoing>flow_7</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:exclusiveGateway id="gateway_accelerated" name="Zrychlený režim?">
      <bpmn:incoming>flow_7</bpmn:incoming>
      <bpmn:outgoing>flow_accelerated</bpmn:outgoing>
      <bpmn:outgoing>flow_standard</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="task_accelerated_processing" name="Zpracovat zrychleně">
      <bpmn:incoming>flow_accelerated</bpmn:incoming>
      <bpmn:outgoing>flow_8</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="task_standard_processing" name="Standardní zpracování">
      <bpmn:incoming>flow_standard</bpmn:incoming>
      <bpmn:outgoing>flow_9</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Events -->
    <bpmn:endEvent id="end_accelerated" name="Zrychlená implementace">
      <bpmn:incoming>flow_8</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:endEvent id="end_standard" name="Předat k schválení">
      <bpmn:incoming>flow_9</bpmn:incoming>
      <bpmn:incoming>flow_timeout</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Boundary Event -->
    <bpmn:boundaryEvent id="timeout_completion" name="7 dní timeout" attachedToRef="task_request_completion">
      <bpmn:outgoing>flow_timeout</bpmn:outgoing>
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration>P7D</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:boundaryEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="flow_1" sourceRef="start_client_request" targetRef="task_document_initiative"/>
    <bpmn:sequenceFlow id="flow_2" sourceRef="start_market_opportunity" targetRef="task_document_initiative"/>
    <bpmn:sequenceFlow id="flow_3" sourceRef="start_new_emission" targetRef="task_document_initiative"/>
    <bpmn:sequenceFlow id="flow_4" sourceRef="start_unique_idea" targetRef="task_document_initiative"/>
    <bpmn:sequenceFlow id="flow_5" sourceRef="task_document_initiative" targetRef="task_submit_initiative"/>
    <bpmn:sequenceFlow id="flow_6" sourceRef="task_submit_initiative" targetRef="gateway_completeness"/>
    <bpmn:sequenceFlow id="flow_complete" name="Ano" sourceRef="gateway_completeness" targetRef="task_archive_initiative"/>
    <bpmn:sequenceFlow id="flow_incomplete_check" name="Ne" sourceRef="gateway_completeness" targetRef="task_request_completion"/>
    <bpmn:sequenceFlow id="flow_incomplete" sourceRef="task_request_completion" targetRef="task_document_initiative"/>
    <bpmn:sequenceFlow id="flow_7" sourceRef="task_archive_initiative" targetRef="gateway_accelerated"/>
    <bpmn:sequenceFlow id="flow_accelerated" name="Ano" sourceRef="gateway_accelerated" targetRef="task_accelerated_processing"/>
    <bpmn:sequenceFlow id="flow_standard" name="Ne" sourceRef="gateway_accelerated" targetRef="task_standard_processing"/>
    <bpmn:sequenceFlow id="flow_8" sourceRef="task_accelerated_processing" targetRef="end_accelerated"/>
    <bpmn:sequenceFlow id="flow_9" sourceRef="task_standard_processing" targetRef="end_standard"/>
    <bpmn:sequenceFlow id="flow_timeout" sourceRef="timeout_completion" targetRef="end_standard"/>
    
    <!-- Data Objects -->
    <bpmn:dataObjectReference id="data_initiative_document" name="Dokument podnětu" dataObjectRef="DataObject_1"/>
    <bpmn:dataObject id="DataObject_1"/>
    
    <bpmn:dataObjectReference id="data_product_list" name="Produktový list" dataObjectRef="DataObject_2"/>
    <bpmn:dataObject id="DataObject_2"/>
    
    <!-- Data Associations -->
    <bpmn:dataOutputAssociation id="data_out_1">
      <bpmn:sourceRef>task_document_initiative</bpmn:sourceRef>
      <bpmn:targetRef>data_initiative_document</bpmn:targetRef>
    </bpmn:dataOutputAssociation>
    
    <bpmn:dataInputAssociation id="data_in_1">
      <bpmn:sourceRef>data_product_list</bpmn:sourceRef>
      <bpmn:targetRef>gateway_accelerated</bpmn:targetRef>
    </bpmn:dataInputAssociation>
  </bpmn:process>
  
  <!-- DI Section -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collaboration_ProductInitiation">
      <bpmndi:BPMNShape id="Pool_ProductInitiation_di" bpmnElement="Pool_ProductInitiation" isHorizontal="true">
        <dc:Bounds x="100" y="50" width="1400" height="400"/>
