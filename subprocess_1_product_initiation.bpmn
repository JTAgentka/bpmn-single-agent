        <di:waypoint x="350" y="98"/>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="flow_4_di" bpmnElement="flow_4">
        <di:waypoint x="286" y="148"/>
        <di:waypoint x="350" y="148"/>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="flow_5_di" bpmnElement="flow_5">
