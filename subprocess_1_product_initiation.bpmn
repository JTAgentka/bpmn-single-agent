      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_ODIC_di" bpmnElement="Lane_ODIC" isHorizontal="true">
        <dc:Bounds x="130" y="200" width="1370" height="250"/>
      </bpmndi:BPMNShape>
      
      <!-- Start Events in Initiator Lane -->
      <bpmndi:BPMNShape id="start_client_request_di" bpmnElement="start_client_request">
