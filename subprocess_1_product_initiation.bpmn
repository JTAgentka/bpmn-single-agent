      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="task_accelerated_processing_di" bpmnElement="task_accelerated_processing">
        <dc:Bounds x="950" y="220" width="100" height="80"/>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="task_standard_processing_di" bpmnElement="task_standard_processing">
        <dc:Bounds x="950" y="330" width="100" height="80"/>
